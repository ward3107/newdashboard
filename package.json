{
  "name": "ai-student-analysis-dashboard",
  "version": "1.0.1",
  "description": "AI-powered student analysis dashboard with Claude AI integration",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@sentry/browser": "^10.32.1",
    "@sentry/react": "^10.32.1",
    "@tanstack/react-query": "^5.90.2",
    "@types/crypto-js": "^4.2.2",
    "@types/react-window": "^1.8.8",
    "axios": "^1.6.2",
    "canvas-confetti": "^1.9.4",
    "chart.js": "^4.5.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "firebase": "^12.5.0",
    "firebase-admin": "^13.5.0",
    "follow-redirects": "^1.15.11",
    "framer-motion": "^12.23.22",
    "html2canvas": "^1.4.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.544.0",
    "react": "18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^16.1.0",
    "react-router-dom": "^6.30.1",
    "react-window": "^2.2.1",
    "recharts": "^3.2.1",
    "web-vitals": "^5.1.0",
    "zod": "^4.1.12"
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run lint:errors && npm run typecheck",
    "build": "vite build",
    "build:prod": "vite build",
    "build:analyze": "vite build --mode production && node analyze-bundle.js",
    "preview": "vite preview",
    "clean": "rimraf dist coverage reports .turbo",
    "typecheck": "tsc --noEmit --pretty",
    "lint": "eslint .",
    "lint:errors": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --reporter=dot",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:int": "vitest run -c vitest.integration.config.ts",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "test:offline": "playwright test tests/e2e/offline-assessment.spec.ts --headed",
    "a11y": "node scripts/run-a11y.cjs",
    "perf": "node scripts/run-perf.cjs",
    "scan:deps": "npm audit --audit-level=moderate && npx better-npm-audit audit",
    "scan:security": "node scripts/security-scan.cjs",
    "security:audit": "bash scripts/security-audit.sh",
    "security:install-hook": "cp scripts/pre-commit-hook.sh .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit && echo 'Pre-commit hook installed successfully!'",
    "security:check-secrets": "bash scripts/pre-commit-hook.sh",
    "precommit": "npm run lint && npm run typecheck && npm run test",
    "ci": "npm run clean && npm run lint && npm run typecheck && npm run test:coverage && npm run e2e && npm run a11y && npm run scan:deps",
    "ci:full": "npm run clean && npm run lint && npm run typecheck && npm run test:coverage && npm run test:int && npm run e2e && npm run a11y && npm run scan:deps && npm run security:audit",
    "reports": "node scripts/generate-reports.cjs",
    "start": "node server.js"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/canvas-confetti": "^1.9.0",
    "@types/node": "^24.9.2",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.8.0",
    "better-npm-audit": "^3.7.3",
    "chrome-launcher": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^2.1.0",
    "globals": "^16.4.0",
    "happy-dom": "^20.0.5",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "msw": "^2.0.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.6",
    "prettier": "^3.2.0",
    "rimraf": "^5.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.1.10",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^3.2.4",
    "workbox-window": "^7.3.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "main": "analyze-bundle.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ward3107/newdashboard.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ward3107/newdashboard/issues"
  },
  "homepage": "https://github.com/ward3107/newdashboard#readme"
}
